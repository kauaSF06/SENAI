programa {
  funcao inicio() {
    inteiro num1, divisores=0, i
    escreva("escreva um numero\n")
    leia(num1)
    se(num1<2){
      escreva("o numero nao e primo")}
      para(i = 2; i < num1; i++)
     se (num1 %i ==0){
      divisores++
     }
    se(divisores ==0){
      escreva("o numero ",num1," e primo")
    }
    senao
    escreva("o numero ",num1," nao e primo")

  }
}
